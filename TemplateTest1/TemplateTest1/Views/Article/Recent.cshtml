@{
    Layout = null;
}
@model  TemplateTest1.Models.RecentDateModel
<div class="posts-wrap">
    <h3>Recent Posts</h3>
    <ul>
        @foreach (var item in Model.Items)
        {
            <li>
                <p>@Html.ActionLink(item.Text, "Index", "Home", new { seoUrl = item.Text }, null)</p>
                <p class="date">@item.Date.ToString("D")</p>
            </li>
        }
    </ul>
</div>